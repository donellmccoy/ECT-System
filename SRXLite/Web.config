    machine.config.comments usually located in 
    \Windows\Microsoft.Net\Framework\v2.x\Config 
  
  *** Aspnet.config settings for impersonation ***
  \Windows\Microsoft.Net\Framework\v2.x\Aspnet.config
  <runtime>
    <legacyUnhandledExceptionPolicy enabled="false" />
    <legacyImpersonationPolicy enabled="false"/>
    <alwaysFlowImpersonationPolicy enabled="true"/>
    <SymbolReadingPolicy enabled="1" />
  </runtime>
-->
<configuration>
  <appSettings>
    <add key="PrintButton" value="False"/>
    <add key="WaterMark" value="For Official Use Only"/>
    <add key="Environment" value="DEV"/>
    <add key="EncryptionEnabled" value="1"/>
    <add key="EncryptionKey" value="srxlitedev"/>
    <add key="FileSizeUploadLimit" value="6291456"/>
    <add key="InitialFileSizeUploadLimit" value="5242880"/>
    <add key="ChartImageHandler" value="storage=file;timeout=20;dir=c:\Temp\"/>
  </appSettings>
  <connectionStrings>
    <remove name="LocalSqlServer" />
    <add name="SRXLite" connectionString="Server=localhost;Database=SRXLite;Integrated Security=True;" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <!-- Set Option Strict On -->
  <system.codedom>
    <compilers>
      <compiler language="vb;vbs;visualbasic;vbscript" extension=".vb" type="Microsoft.VisualBasic.VBCodeProvider, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" compilerOptions="/optionstrict+" warningLevel="4">
        <providerOption name="CompilerVersion" value="v4.0"/>
        <providerOption name="OptionInfer" value="true"/>
        <providerOption name="WarnAsError" value="false"/>
      </compiler>
    </compilers>
  </system.codedom>
  <system.net>
    <mailSettings>
      <smtp>
        <network host="localhost"/>
      </smtp>
    </mailSettings>
  </system.net>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5.1" />
      </system.Web>
  -->
  <system.web>
    <!-- set debug="false" for prod-->
    <compilation debug="true" strict="true" explicit="true" targetFramework="4.8.1">
      <assemblies>
        <add assembly="ABCpdf, Version=12.5.1.0, Culture=neutral, PublicKeyToken=A7A0B3F5184F2169"/>
