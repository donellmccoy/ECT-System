  <!-- Ensure security headers for static content directories -->
  <location path="App_Themes">
    <system.webServer>
      <httpProtocol>
        <customHeaders>
          <add name="X-Content-Type-Options" value="nosniff"/>
          <add name="X-Frame-Options" value="SAMEORIGIN"/>
          <add name="Cache-Control" value="public, max-age=604800"/>
        </customHeaders>
      </httpProtocol>
      <staticContent>
        <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="7.00:00:00" setEtag="true"/>
      </staticContent>
    </system.webServer>
  </location>
  
