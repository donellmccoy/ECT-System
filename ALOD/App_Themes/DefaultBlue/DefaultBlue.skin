<%-- Menus --%>
<asp:Menu runat="server" SkinId="MainMenu" Orientation="Horizontal" StaticEnableDefaultPopOutImage="False" 
    DynamicEnableDefaultPopOutImage="False" DynamicHorizontalOffset="1" StaticSubMenuIndent="0" ReneringMode="Table">
    <StaticMenuStyle CssClass="MainMenuStaticStyle" />
    <StaticMenuItemStyle CssClass="MainMenuStaticItemStyle" ItemSpacing="0px" />
    <DynamicHoverStyle CssClass="MainMenuDynamicItemHoverStyle" />
    <DynamicMenuItemStyle CssClass="MainMenuDynamicItemStyle" />
    <StaticHoverStyle CssClass="MainMenuStaticItemHoverStyle" />
</asp:Menu>

<%-- Grid Views --%>
<asp:GridView runat="server" CssClass="gridViewMain" GridLines="Horizontal" CellPadding="0">
