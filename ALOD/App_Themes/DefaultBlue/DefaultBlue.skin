   <PagerStyle CssClass="gridViewPager" />
   <RowStyle CssClass="gridViewRow" />
   <EditRowStyle CssClass="gridViewEdit" />
   <SelectedRowStyle CssClass="gridViewSelected" />
</asp:GridView>

<asp:GridView runat="server" SkinId="TestGV" CssClass="gridViewMain" GridLines="Horizontal" CellPadding="0">
   <AlternatingRowStyle CssClass="newGridViewAlternateRow" />
   <HeaderStyle CssClass="newGridViewHeader" />
   <PagerStyle CssClass="gridViewPager" />
   <RowStyle CssClass="newGridViewRow" />
   <EditRowStyle CssClass="gridViewEdit" />
   <SelectedRowStyle CssClass="gridViewSelected" />
</asp:GridView>