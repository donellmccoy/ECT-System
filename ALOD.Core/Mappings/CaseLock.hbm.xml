<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="ALOD.Core"
                   namespace="ALOD.Core.Domain.Workflow">

	<class name="CaseLock" table="core_workflowLocks">
		<id name="Id" column="id">
			<generator class="identity" />
		</id>

		<property name="ReferenceId" column="refId" />
		<property name="ModuleType" column="module" />
		<property name="LockTime" column="lockTime" />
		<property name="UserId" column="userId" />
	</class>
</hibernate-mapping>