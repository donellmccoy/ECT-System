		<property name="PostalCode" column="Postal_Code" />
		<!-- <property name="Scheduling" column="Scheduling_YN" type="YesNo" /> -->
		<property name="State" column="State" />
		<property name="TimeZone" column="Time_Zone" />
		<property name="Uic" column="Uic" />
		<property name="UnitDet" column="Unit_Det" />
		<property name="UnitKind" column="Unit_Kind" />
		<property name="UnitNumber" column="Unit_Nbr" />
		<property name="UnitType" column="Unit_Type" />
    <property name="InActive" column="inActive" />
    <property name="UserModified" column="UserModified" />
	  <property name="IsCollocated" column="IsCollocated" />
		<many-to-one name="GainingCommand" column="GAINING_COMMAND_CS_ID" class="Unit" />
    <many-to-one name="ParentUnit" column="CS_ID_PARENT" class="Unit" />
	</class>
</hibernate-mapping>