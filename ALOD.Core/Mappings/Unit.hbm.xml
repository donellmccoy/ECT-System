<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="ALOD.Core"
                   namespace="ALOD.Core.Domain.Users">

	<class name="Unit" table="command_struct">
		<id name="Id" column="cs_id">
			<generator class="identity" />
		</id>

		<property name="Name" column="long_name" />
		<property name="Address1" column="Address1" />
		<property name="Address2" column="Address2" />
		<property name="BaseCode" column="base_code" />
		<property name="City" column="City" />
		<property name="CommandCode" column="Command_Code" />
		<property name="CommandStructUTC" column="Command_Struct_UTC" />
		<property name="Component" column="Component" />
		<property name="Country" column="Country" />
		<property name="CreatedDate" column="created_date" />
    <property name="CommandStructLevel" column="cs_level" />
		<property name="CommandStructOperationType" column="cs_oper_type" />
		<property name="Email" column="e_mail" />
		<!-- <property name="GeographicLocation" column="geo_loc" /> -->
		<property name="MedicalService" column="Medical_Service" />
		<property name="ModifiedDate" column="modified_date" />
		<property name="MrdssDocDate" column="mrdss_doc_date" />
		<property name="MrdssDocId" column="mrdss_doc_id" />
		<property name="MrdssDocReview" column="mrdss_doc_review" />
		<property name="MrdssKind" column="mrdss_kind" />
		<property name="PasCode" column="pas_code" />
